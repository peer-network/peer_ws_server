    "chai": "^4.3.4",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.3.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2"
