  "description": "An example of setting up subscriptions with Apollo Server 4",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "postinstall": "npm run compile",
    "compile": "tsc",
    "generate-server": "graphql-codegen --config ./src/infrastructure/gql/codegen-config/codegen-server.yml",
