    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
