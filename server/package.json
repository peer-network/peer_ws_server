    "@project-serum/anchor": "^0.26.0",
    "@solana/web3.js": "^1.98.0",
    "@types/dotenv": "^6.1.1",
    "apollo-server": "^3.13.0",
    "axios": "^1.8.4",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-graphql": "^0.12.0",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "graphql-tools": "^9.0.18",
    "jwt-decode": "^4.0.0",
    "reaact": "^0.0.1-security",
    "react": "^19.1.0",
    "rehackt": "^0.1.0",
    "winston": "^3.11.0",
    "zod": "^3.24.3",
